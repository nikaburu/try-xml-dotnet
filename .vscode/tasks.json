{
    "version": "2.0.0",
    "tasks": [
        {
            "taskName": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/Try.Xml.Console/Try.Xml.Console.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "taskName": "run",
            "command": "dotnet",
            "type": "process",
            "args": [
                "run",
                "-p",
                "${workspaceFolder}/src/Try.Xml.Console/Try.Xml.Console.csproj"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}